<% @photos.each do |photo| %>
  <%= link_to image_tag(photo.image.url, :class => 'gallery_image mx-auto'), photo.image.url, target: '_blank' %>
  <% if admin_signed_in? %>
    <div class='text-center'>
      <%= button_to 'delete', "/photos/#{photo.id}", method: :delete, :class => 'admin_button' %>
    </div>
  <% end %>
<% end %>
